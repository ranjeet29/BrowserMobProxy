h5 class=\"fg-emerald\">3.0</h5>\n      <p class=\"text-accent fg-gray\">You are currently on the <b>beta</b> update channel</p>\n      <h5 class=\"fg-gray\">Maintained by</h5>\n      <ul class=\"numeric-list\">\n          <li>Ankur Srivastava(ankur.srivastava@cavisson.com)</li>\n      </ul>\n      <h5 class=\"fg-gray\">Contributors</h5>\n      <ol class=\"numeric-list\">\n          <li>Vishal Singhal(vishal.singhal@cavisson.com)</li>\n          <li>Niraj Panda(niraj.panda@cavisson.com)</li>\n          <li>Arun Yadav(arun.yadav@cavisson.com)</li>\n      </ol>\n      <div class=\"padding10\" style=\"background-color: #eeeeee; margin-left: -12px; margin-right: -12px; margin-bottom: -12px\">\n      <p class=\"fg-lightRed\">&copy; 2016-17 | Automation Team | Cavisson Systems Inc</p>\n      </div>\n  </div>\n     <span class=\"dialog-close-button\"></span>\n</div>\n<div class=\"grid responsive margin40 no-margin-left no-margin-right\">\n\t<div class=\"row cells12\">\n\t\t<div class=\"cell colspan2\">\n\t\t\t<div class=\"cell padding10 no-padding-right\" height=\"100%\">\t\n\t\t\t\t\n\t\t\t</div>\n\t\t</div>\n\t\t<div class=\"cell colspan10\">\n\t\t\t\n<div class=\"grid margin20 padding40\">\n\t<div class=\"row cells12\">\n\t\t<div class=\"cell colspan2\"></div>\n        <div class=\"cell colspan4 margin10 \" style=\"border: 0px solid #EEEEEE\">\n            \n            <div class=\"padding10\" style=\"border: 1px solid #EEEEEE; box-shadow: 0 16px 16px -16px gray; background-color: white\"><p class=\"header text-accent fg-gray\" align=\"left\" style=\"border-bottom: 1px solid #eeeeee; padding-bottom: 15px; padding-left: 5px;\">Sign in to your account</p>\n                <form action=\"/account/service\" method=\"post\">\n\t\t\t\t\t<div class=\"input-control modern text iconic\">\n\t\t\t\t\t\t<input type=\"text\" name=\"username\">\n\t\t\t\t\t\t\t<span class=\"label\">Username</span>\n\t\t\t\t\t\t    <span class=\"informer\">Please enter you username</span>\n\t\t\t\t\t\t    <span class=\"placeholder\">Username</span>\n\t\t\t\t\t\t    <span class=\"icon mif-user\"></span>\n\t\t\t\t\t</div>\n\t\t\t\t\t<div class=\"input-control modern text iconic\" data-role=\"input\">\n\t\t\t\t\t\t<input type=\"password\" name=\"password\">\n\t\t\t\t\t\t\t<span class=\"label\">Password</span>\n\t\t\t\t\t\t    <span class=\"informer\">Please enter your password</span>\n\t\t\t\t\t\t   \t <span class=\"placeholder\">Password</span>\n\t\t\t\t\t\t    <span class=\"icon mif-lock\"></span>\n\t\t\t\t\t</div>\n\t\t\t\t\t<br><br>\n\t\t\t\t\t&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;\n                    <div class=\"padding10\" style=\"border:0px solid #EEEEEE\">\n\t\t\t\t\t<button class=\"button primary\">\n\t\t\t\t\t\t<span class=\"icon mif-enter\"></span>&nbsp;Sign In</button>\n\t\t\t\t\t<a href=\"/welcome\" class=\"margin20 link fg-lightRed\">Cancel</a>\n\n                    </div>\n\t\t\t\t</form>\n\t\t\t</div>\n\t\t\t</div>\n\t\t</div>\n\t\t<div class=\"cell colspan4\"></div>\n\t</div>\n</div>\n\n\t\n\t\t</div>\n\t</div>\n</div>\n\n<script type=\"text/javascript\">\n    /* preload variable declaration start */\n    var cpsChart = null; \n    var hpsChart = null; \n    var tputChart = null;\n    var smokeChart = null; \n    var regChart = null; \n    var metaData = null;\n\n    function get2dContext(id){\n        return document.getElementById(id).getContext(\"2d\");\n    }\n\n    function drawLineChart(context, data, options){\n        return new Chart(context).Line(data, options);\n    }\n    \n    function drawBarChart(context, data, options){\n        return new Chart(context).Bar(data, options);\n    }\n\n    var labels = [  ]\n                    \n    var cpsData = {\n        labels: labels, \n        datasets: [\n            {\n                label: \"My First dataset\",\n                fillColor: \"rgba(220, 220, 220, 0.2)\",\n                strokeColor: \"rgba(151, 187, 205, 1)\",\n                pointColor: \"rgba(151, 187, 205, 1)\",\n                pointStrokeColor: \"#fff\",\n                pointHighlightFill: \"#fff\",\n                pointHighlightStroke: \"rgba(151, 187, 205, 1)\",\n                data: [ ]\n            },\n            {\n                label: \"My Second dataset\",\n                fillColor: \"rgba(253, 153, 0, 0.2)\",\n                strokeColor: \"rgba(253, 153, 0, 1)\",\n                pointColor: \"rgba(253, 153, 0, 1)\",\n                pointStrokeColor: \"#fff\",\n                pointHighlightFill: \"#fff\",\n                pointHighlightStroke: \"rgba(151, 187, 205, 1)\",\n                data: [ \n                    \n                ]\n            }\n        ]\n    };\n\n    var hpsData = {\n        labels: labels, \n        datasets: [\n            {\n                label: \"My First dataset\",\n                fillColor: \"rgba(220, 220, 220, 0.2)\",\n                strokeColor: \"rgba(151, 187, 205, 1)\",\n                pointColor: \"rgba(151, 187, 205, 1)\",\n                pointStrokeColor: \"#fff\",\n                pointHighlightFill: \"#fff\",\n                pointHighlightStroke: \"rgba(151, 187, 205, 1)\",\n                data: [ ]\n            },\n            {\n                label: \"My Second dataset\",\n                fillColor: \"rgba(253, 153, 0, 0.2)\",\n                strokeColor: \"rgba(253, 153, 0, 1)\",\n                pointColor: \"rgba(253, 153, 0, 1)\",\n                pointStrokeColor: \"#fff\",\n                pointHighlightFill: \"#fff\",\n                pointHighlightStroke: \"rgba(151, 187, 205, 1)\",\n                data: [ \n                    \n                ]\n            }\n        ]\n    };\n    \n    var throughputData = {\n        labels: labels, \n        datasets: [\n            {\n                label: \"My First dataset\",\n                fillColor: \"rgba(220, 220, 220, 0.2)\",\n                strokeColor: \"rgba(151, 187, 205, 1)\",\n                pointColor: \"rgba(151, 187, 205, 1)\",\n                pointStrokeColor: \"#fff\",\n                pointHighlightFill: \"#fff\",\n                pointHighlightStroke: \"rgba(151, 187, 205, 1)\",\n                data: [ ]\n            },\n            {\n                label: \"My Second dataset\",\n                fillColor: \"rgba(253, 153, 0, 0.2)\",\n                strokeColor: \"rgba(253, 153, 0, 1)\",\n                pointColor: \"rgba(253, 153, 0, 1)\",\n                pointStrokeColor: \"#fff\",\n                pointHighlightFill: \"#fff\",\n                pointHighlightStroke: \"rgba(151, 187, 205, 1)\",\n                data: [ \n                    ]\n            }\n        ]\n    };\n    \n    smokelabels = [];\n    \n    var smokeData = {\n        labels: smokelabels, \n        datasets: [\n            {\n                label: \"Total\",\n                fillColor: \"rgb(133, 163, 194)\",\n                strokeColor: \"rgb(133, 163, 194)\",\n                highlightFill: \"rgb(133, 163, 194)\",\n                highlightStroke: \"rgb(133, 163, 194)\",\n                data: []\n            },\n            \n            {\n                label: \"Passed\",\n                fillColor: \"rgb(128, 204, 153)\",\n                strokeColor: \"rgb(128, 204, 153)\",\n                highlightFill: \"rgb(128, 204, 153)\",\n                highlightStroke: \"rgb(128, 204, 153)\",\n                data: []\n            },\n            {\n                label: \"Failed\",\n                fillColor: \"rgb(255, 102, 102)\",\n                strokeColor: \"rgb(255, 102, 102)\",\n                highlightFill: \"rgb(255, 102, 102)\",\n                highlightStroke: \"rgb(255, 102, 102)\",\n                data: []\n            },\n        ]\n    };\n\n    reglabels = [];\n    \n    var regData = {\n        labels: reglabels, \n        datasets: [\n        {\n                label: \"Total\",\n                fillColor: \"rgb(133, 163, 194)\",\n                strokeColor: \"rgb(133, 163, 194)\",\n                highlightFill: \"rgb(133, 163, 194)\",\n                highlightStroke: \"rgb(133, 163, 194)\",\n                data: []\n            },\n\n            {\n                label: \"Passed\",\n                fillColor: \"rgb(128, 204, 153)\",\n                strokeColor: \"rgb(128, 204, 153)\",\n                highlightFill: \"rgb(128, 204, 153)\",\n                highlightStroke: \"rgb(128, 204, 153)\",\n                data: []\n            },\n            {\n                label: \"Failed\",\n                fillColor: \"rgb(255, 102, 102)\",\n                strokeColor: \"rgb(255, 102, 102)\",\n                highlightFill: \"rgb(255, 102, 102)\",\n                highlightStroke: \"rgb(255, 102, 102)\",\n                data: []\n            },\n\n        ]\n    };\n    /* preload variable declaration end */\n\n    /* ready function starts */\n    $(document).ready(function() {\n        var cpsContext = get2dContext('cps'); \n        var hpsContext = get2dContext('hps'); \n        var throughputContext = get2dContext('throughput'); \n        var smokeContext = get2dContext('smoke'); \n        var regressionContext = get2dContext('regression');\n       \n        cpsChart = drawLineChart(cpsContext, cpsData, {  \n            scaleLabel: \"<%=Number(value)/1000 + ' K' %>\",\n            pointHitDetectionRadius : 1\n        });\n        hpsChart = drawLineChart(hpsContext, hpsData, {\n            scaleLabel: \"<%=Number(value)/1000 + ' K' %>\",\n            pointHitDetectionRadius : 1\n        }); \n        tputChart = drawLineChart(throughputContext, throughputData, {\n            scaleLabel: \"<%=Number(value)/1000 + ' Mbits/s' %>\",\n            pointHitDetectionRadius : 1\n        }); \n        \n        smokeChart = drawBarChart(smokeContext, smokeData, \n            {\n             pointHitDetectionRadius : 1,\n             tooltipTitleFontSize: 14,\n            }); \n        \n        regChart = drawBarChart(regressionContext, regData, \n                {\n                 pointHitDetectionRadius : 1,\n                 tooltipTitleFontSize: 14,\n                });\n\n        /* On click action */\n        $(\"#loadGraph\").click(function(){\n           var version = $('#version').val();           \n           var limit = $('#limit').val();         \n           if ( version == \"Select Release\" && limit == \"Limit Builds To\"){\n               notify('Error - Not loading graphs', 'Please select release and limit information', '', '3000');\n               return ;\n           }\n           \n           if ( version == \"Select Release\" ){\n                notify('Error - Not loading graphs', 'Please select release', '', '3000');\n                return \n           }\n           \n           if ( limit == \"Limit Builds To\" ) {\n               notify('Error - Not loading graphs', 'Please select limit', '', '3000');\n               return ;\n           }\n           \n           /* Setting metadata here, so that this information can be accessed from some other code */ \n           metaData = {\n               version: version,\n               limit: limit\n           };\n           plot_performance_data(version, limit);\n           //Moved this code to method plot(). Header will be updated only graphs contains data\n           //$('#trendHdr').html('Trends - Last ' + limit + ' builds for R' + version);\n \n        });\n    });\n\n    function destroyOldChartRefs(){\n    if (cpsChart != null && hpsChart != null && tputChart != null) {\n        cpsChart.destroy();\n        hpsChart.destroy();\n        tputChart.destroy();\n        smokeChart.destroy();\n        regChart.destroy();\n        debug('Destroyed old chart references');\n        }\n    }\n\n\nfunction plot(gdata){\n   if (gdata.builds == null || gdata.builds == \"\"){\n       debug('No data available. gdata.builds=[' + gdata.builds + ']');\n       notify('Warning', 'No data available for selected values. Not refreshing graphs.', 'default', '5000');\n       return; \n   }\n\n   // We need to rebuild get_perf_data module to cater java perf data also\n   cps = get_perf_data(gdata.builds, gdata.cps, gdata.jcps);\n   hps = get_perf_data(gdata.builds, gdata.hps, gdata.jhps); \n   throughput = get_perf_data(gdata.builds, gdata.throughput, gdata.jthroughput);\n   smoke = get_test_data(gdata.smoke.build, gdata.smoke.failed, gdata.smoke.passed, gdata.smoke.total)\n   reg = get_test_data(gdata.reg.build, gdata.reg.failed, gdata.reg.passed, gdata.reg.total)\n\n   var ct1 = get2dContext('cps');\n   var ct2 = get2dContext('hps'); \n   var ct3 = get2dContext('throughput');\n   var ct4 = get2dContext('smoke');\n   var ct5 = get2dContext('regression');\n\n   /* Destroy old chart reference and data before plotting new graphs */\n   destroyOldChartRefs();\n\n   cpsChart = drawLineChart(ct1, cps, { \n        scaleLabel: \"<%=Number(value)/1000 + ' K' %>\",\n        pointHitDetectionRadius : 1\n   });\n   hpsChart = drawLineChart(ct2, hps, {\n        scaleLabel: \"<%=Number(value)/1000 + ' K' %>\",\n        pointHitDetectionRadius : 1\n   }); \n\n   tputChart = drawLineChart(ct3, throughput, {\n       scaleLabel: \"<%=Number(value)/1000 + ' Mbits/s' %>\",\n       pointHitDetectionRadius : 1\n   }); \n   \n   smokeChart = drawBarChart(ct4, smoke, \n           {\n            pointHitDetectionRadius : 1,\n            tooltipTitleFontSize: 14,\n           }); \n   \n   regChart = drawBarChart(ct5, reg, \n           {\n            pointHitDetectionRadius : 1,\n            tooltipTitleFontSize: 14,    \n           });\n   \n   $('#trendHdr').html('Trends - Last ' + metaData.limit  + ' builds for R' + metaData.version);\n} \n\n\nfunction get_perf_data(label, metric1, metric2){\n    var data = {\n        labels: label, \n        datasets: [\n            {\n                label: \"My First dataset\",\n                fillColor: \"rgba(220, 220, 220, 0.2)\",\n                strokeColor: \"rgba(151, 187, 205, 1)\",\n                pointColor: \"rgba(151, 187, 205, 1)\",\n                pointStrokeColor: \"#fff\",\n                pointHighlightFill: \"#fff\",\n                pointHighlightStroke: \"rgba(151, 187, 205, 1)\",\n                data: metric1 \n            },\n            {\n                label: \"My Second dataset\",\n                fillColor: \"rgba(253, 153, 0, 0.2)\",\n                strokeColor: \"rgba(253, 153, 0, 1)\",\n                pointColor: \"rgba(253, 153, 0, 1)\",\n                pointStrokeColor: \"#fff\",\n                pointHighlightFill: \"#fff\",\n                pointHighlightStroke: \"rgba(151, 187, 205, 1)\",\n                data: metric2 \n            }\n        ]\n    };\n    \n    return data;\n}\n\nfunction get_test_data(label, failed, passed, total){\n    var testData = {\n            labels: label, \n            datasets: [\n            {\n                    label: \"Total\",\n                    fillColor: \"rgb(133, 163, 194)\",\n                    strokeColor: \"rgb(133, 163, 194)\",\n                    highlightFill: \"rgb(133, 163, 194)\",\n                    highlightStroke: \"rgb(133, 163, 194)\",\n                    data: total \n                },\n\n                {\n                    label: \"Passed\",\n                    fillColor: \"rgb(128, 204, 153)\",\n                    strokeColor: \"rgb(128, 204, 153)\",\n                    highlightFill: \"rgb(128, 204, 153)\",\n                    highlightStroke: \"rgb(128, 204, 153)\",\n                    data: passed \n                },\n                {\n                    label: \"Failed\",\n                    fillColor: \"rgb(255, 102, 102)\",\n                    strokeColor: \"rgb(255, 102, 102)\",\n                    highlightFill: \"rgb(255, 102, 102)\",\n                    highlightStroke: \"rgb(255, 102, 102)\",\n                    data: failed\n                },\n\n            ]\n        };\n     return testData\n}\n\n\nfunction ajaxCall(method, url, callb